<p id="notice"><%= notice %></p>

<h1>Meetups</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Location</th>
      <th>Lat</th>
      <th>Lng</th>
      <th>Start time</th>
      <th>End time</th>
      <th>Game</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @meetups.each do |meetup| %>
      <tr>
        <td><%= meetup.title %></td>
        <td><%= meetup.location %></td>
        <td><%= meetup.lat %></td>
        <td><%= meetup.lng %></td>
        <td><%= meetup.start_time %></td>
        <td><%= meetup.end_time %></td>
        <td><%= meetup.game %></td>
        <td><%= meetup.user %></td>
        <td><%= link_to 'Show', meetup %></td>
        <td><%= link_to 'Edit', edit_meetup_path(meetup) %></td>
        <td><%= link_to 'Destroy', meetup, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Meetup', new_meetup_path %>
